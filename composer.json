{
    "name": "pazuzu156/anubisbot",
    "description": "A Discord bot written in PHP",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Kaleb Klein",
            "email": "klein.jae@gmail.com"
        }
    ],
    "keywords": ["bot", "discord", "php"],
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "require": {
        "php": ">=5.6",
        "team-reflex/discord-php": "^4.0",
        "vlucas/phpdotenv": "^2.4",
        "php-curl-class/php-curl-class": "^7.2",
        "symfony/console": "^3.2",
        "symfony/var-dumper": "^3.2",
        "illuminate/filesystem": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Core\\": "core/"
        },
        "files": [ "core/Utils/helpers.php" ]
    },
    "scripts": {
        "post-root-package-install": [ "php -r \"file_exists('.env') || copy('.env.example', '.env');\"" ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3-dev",
            "dev-develop": "0.4-dev"
        }
    }
}
